{
  "name": "css-var-extract-cli",
  "version": "0.1.0",
  "description": "Use CSS vars type-safely for CSS-in-JS",
  "keywords": [
    "css",
    "css-in-js",
    "react",
    "typescript",
    "codegen",
    "generation",
    "cli"
  ],
  "homepage": "https://github.com/sya-ri/css-var-extract#readme",
  "bugs": {
    "url": "https://github.com/sya-ri/css-var-extract/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sya-ri/css-var-extract.git"
  },
  "license": "MIT",
  "author": "sya-ri",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "bin": {
    "cve": "bin/cve.cjs"
  },
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "chokidar": "3.6.0",
    "css-var-extract": "0.1.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "17.0.33",
    "css-var-extract-vite-config": "0.1.0",
    "vite": "5.4.2"
  }
}
