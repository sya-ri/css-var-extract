{
  "name": "css-var-extract",
  "version": "1.0.0",
  "description": "Use CSS vars type-safely for CSS-in-JS",
  "keywords": [
    "css",
    "css-in-js",
    "react",
    "typescript",
    "codegen",
    "generation"
  ],
  "homepage": "https://github.com/sya-ri/css-var-extract#readme",
  "bugs": {
    "url": "https://github.com/sya-ri/css-var-extract/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sya-ri/css-var-extract.git"
  },
  "license": "MIT",
  "author": "sya-ri",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
